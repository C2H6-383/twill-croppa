{
  "name": "c2h6/twill-croppa",
  "description": "Use Croppa as media rendering service in Twill applications",
  "keywords": [
    "croppa",
    "twill",
    "laravel",
    "image",
    "media",
    "twillcms"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/C2H6-383/twill-croppa",
  "autoload": {
    "psr-4": {
      "C2H6\\TwillCroppa\\": "src/"
    },
    "files": [
      "src/Utils.php"
    ]
  },
  "authors": [
    {
      "name": "Ethan Finley IT & Medientechnik",
      "email": "info@ethan-finley.de",
      "homepage": "https://ethan-finley.de"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^11.0",
    "area17/twill": "^3.4",
    "bkwld/croppa": "dev-master"
  },
  "extra": {
    "laravel": {
      "providers": [
        "C2H6\\TwillCroppa\\TwillCroppaServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "10.5.x-dev",
    "orchestra/testbench": "9.x-dev"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/C2H6-383/croppa"
    }
  ]
}
